{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [".+.yml", ".+.yaml"],
      "matchStrings": [
        "(?<depName>.+?)-version: v(?<currentValue>.+)\\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "denoland/deno"
    }
  ]
}
